    <!-- http://www.ontology.audit4sg.org/RelAIEO#online_platform -->

    <owl:Class rdf:about="http://www.ontology.audit4sg.org/RelAIEO#online_platform">
        <rdfs:subClassOf rdf:resource="http://www.ontology.audit4sg.org/RelAIEO#education_user"/>
