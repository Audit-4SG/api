    <owl:Class rdf:about="http://www.ontology.audit4sg.org/RelAIEO#not_listed">
        <rdfs:description>An aspect of socio-technical system that is not part of this ontology.</rdfs:description>
    </owl:Class>
    


    <!-- http://www.ontology.audit4sg.org/RelAIEO#notice_and_consent -->

    <owl:Class rdf:about="http://www.ontology.audit4sg.org/RelAIEO#notice_and_consent">
        <rdfs:subClassOf rdf:resource="http://www.ontology.audit4sg.org/RelAIEO#non_relational_approach"/>
