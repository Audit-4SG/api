    <!-- http://www.ontology.audit4sg.org/RelAIEO#mathematical_criteria -->

    <owl:Class rdf:about="http://www.ontology.audit4sg.org/RelAIEO#mathematical_criteria">
        <rdfs:subClassOf rdf:resource="http://www.ontology.audit4sg.org/RelAIEO#non_relational_approach"/>
